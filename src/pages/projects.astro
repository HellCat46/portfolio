---
import Layout from "../layouts/Layout.astro";
import Project from "../components/Project.astro";
import { TAGS, STATUS } from "../shared.types";
---

<Layout title="Projects | Harshit">
  <Project
    name="Enkidu"
    repolink="https://github.com/HellCat46/Enkidu"
    description="CLI tool to insert data into a database from a CSV file"
    started_at="Jan 4, 2024"
    status={STATUS.Working}
    tags={[TAGS.Java]}
  />
  <Project
    name="Frieren"
    repolink="https://github.com/HellCat46/Frieren"
    description="Frieren is a Discord Bot to manage study notes with some extra features."
    started_at="Nov 1, 2023"
    completed_at="Nov 14, 2023"
    status={STATUS.Complete}
    tags={[TAGS.Postgres, TAGS.Typescript, TAGS.Nodejs]}
  />
  <Project 
    name="Pater"
    repolink="https://github.com/Seikakuna/Pater"
    description="Just Another Link Shortener"
    started_at="Dec 1, 2023"
    status={STATUS.Working}
    tags={[TAGS.CSharp, TAGS.MsSQL]}
  />
  <Project
    name="Portfolio"
    repolink="https://github.com/HellCat46/portfolio"
    description="The Website you are looking at right now."
    started_at="Nov 15,2023"
    completed_at="Dec 6,2023"
    status={STATUS.Complete}
    tags={[TAGS.Typescript, TAGS.Nodejs]}
    livelink="/"
  />
</Layout>
